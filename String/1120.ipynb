   "source": [
    "a, b = input().split()\n",
    "\n",
    "na = len(a)\n",
    "nb = len(b)\n",
    "d = nb - na\n",
    "diff = 0\n",
    "res = 9999999\n",
    "\n",
    "for i in range(d+1):\n",
    "    for j in range(na):\n",
    "        if a[j] != b[i+j]:\n",
    "            diff += 1\n",
    "    if res > diff:\n",
    "        res = diff\n",
    "    diff = 0\n",
    "\n",
    "print(res)\n"
   ]
